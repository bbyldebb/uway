.content {
  display: flex;
  height: 100%;
  align-items: flex-start;
  box-sizing: border-box;
  flex-direction: row;
  margin: 5px;
}

.box1_ {
  display: flex;
  text-align: center;
  /*文本居中*/
  height: 700rpx;
  flex-direction: column;
  width: 30%;
  margin-right: 20px;
}

.box1_info1 {
  margin-top: 0px;
  background-color: #F3EDDA;
  height: 8%;
  line-height: 60px;
}

.box1_info2 {
  border-radius: 2%;
  align-items: center;
  display: flex;
  flex-direction: column;
  margin-top: 0px;
  background-color: #FFFAF0;
  height: 92%;
  line-height: 60px;
}

.info {
  border-radius: 2%;
  border: 10%;
  height: 50px;
  width: 90%;
  line-height: 50px;
  background-color: #BAC6AC;
  margin-top: 5px;
}

.box2_ {
  border-radius: 5%;
  text-align: center;
  /*文字居中*/
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  /*分开*/
  align-items: flex-end;
  width: 70%;
  background-color: #FFFAF0;
}

.box2_view {
  display: flex;
  flex-direction: row;
  margin: 20px;
}

.table {
  border-radius: 17%;
  /*圆角*/
  line-height: 120px;
  min-height: 120px;
  min-width: 100px;
  margin: 20px;
}

.table_action {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

.hook {
  border-radius: 50%;
  /*圆角*/
  background-color: rgb(185, 179, 179);
  line-height: 40px;
  min-height: 40px;
  min-width: 40px;
}

.scroll-view_H {
  white-space: nowrap;
}

.scroll-view-item {
  height: 100rpx;
}

.scroll-view-item_H {
  display: inline-block;
  width: 100%;
  height: 100rpx;
}

.contents {
  width: 250rpx;
  height: 100rpx;
}

