.content {
    display: flex;
    height: 100vh;
    flex-direction: column;
    align-items: center;
}
.control{
    width: 28%;
    height: 6%;
    display: flex;
    align-items:flex-start;
    justify-content: space-between;
}
.btn{
    width: 35%;
    height: 96%;
    margin-bottom: 5px;
    background-color: chocolate;
}
.layout{
    width: 76%;
    height:94%;
    display: flex;
    background-color:#FFFAF0;
    border-radius: 10rpx;
    border:1px solid rgba(146, 136, 136,0.3);
}
.table{
    width: 200px;
    height: 150px;
    border-radius: 17%;
    /*圆角*/
    line-height: 150px;
    min-height: 150px;
    /* margin: 5rpx; */
    background-color: #CF9B69
}